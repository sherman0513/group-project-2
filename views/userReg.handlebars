    <hr class="divider">
    <p id="textSign"> <strong> New to Take A Hike? Sign Up Now!</strong> </p>
    <form id="add" class="formSign">
      <div class="form-group">
        <div class="row">
          <div class="form-group col-sm-6">
            <!-- <label for="inputName" class="control-label">Name</label> -->
            <input type="text" class="form-control" id="fName" placeholder="First Name">
          </div>
          <div class="form-group col-sm-6">
            <!-- <label for="inputName" class="control-label">Name</label> -->
            <input type="text" class="form-control" id="lName" placeholder="Last Name">
          </div>
        </div>

        <div class="form-group">
          {{!-- <label for="inputAddress">Address</label> --}}
          <input type="text" class="form-control" id="inputAddress" placeholder="1234 Main St">
        </div>

        <div class="form-row">
          <div class="form-group col-md-6">
            {{!-- <label for="inputCity">City</label> --}}
            <input type="text" class="form-control" id="inputCity" placeholder="City">
          </div>
          <div class="form-group col-md-4">
            {{!-- <label for="inputState">State</label> --}}
            <select id="inputState" class="form-control">
              <option selected>Choose...</option>
              <option>AK</option>
              <option>AZ</option>
              <option>CA</option>
              <option>CO</option>
              <option>CT</option>
              <option>DE</option>
              <option>FL</option>
              <option>GA</option>
              <option>HI</option>
              <option>ID</option>
              <option>IL</option>
              <option>KS</option>
              <option>MA</option>
              <option>NV</option>
              <option>NM</option>
              <option>UT</option>
              <option>WA</option>
            </select>
          </div>
          <div class="form-group col-md-2">
            {{!-- <label for="inputZip">Zip</label> --}}
            <input type="text" class="form-control" id="inputZip" placeholder="Zip">
          </div>
        </div>
        <div class="form-group">
          <div class="row">
            <div class="form-group col-sm-12">
              <!-- <label for="inputEmail" class="control-label">Email</label> -->
              <input type="email" class="form-control" id="inputEmail" placeholder="Email"
                data-error="Please enter a valid email address." required>
              <div class="help-block with-errors"></div>
            </div>
          </div>
          <div class="form-group">
             {{!-- <label for="inputPassword" class="control-label">Password</label> --> --}}
            
            <div class="row">
              <div class="form-group col-sm-6">
                <input type="password" data-minlength="6" class="form-control" id="inputPassword" placeholder="Password"
                  required>
                &nbsp; <div class="help-block">Min of six characters</div>
              </div>
              <div class="form-group col-sm-6">
                <input type="password" class="form-control" id="inputPasswordConfirm" data-match="#inputPassword"
                  data-match-error="Uh oh, these don't match!" placeholder="Confirm" required>
                <div class="help-block with-errors">
                  <br> 
                  <button id="submitSignUp" type="submit" class="btn btn-primary">Sign Up</button></div>
              </div>
            </div>
          </div>
          {{!-- <div class="form-group">  --}}
          {{!-- <button id="submitSignUp" type="submit" class="btn btn-primary">Sign Up</button> --}}
        </div>
    </form>
